{
    "reporterEnabled": "mochawesome, mocha-junit-reporters",
    "mochaJunitReportersReporterOptions": {
        "mochaFile": "cypress/reports/junit/test_results[hash].xml",
        "toConsole": false
    },
    "mochawesomeReporterOptions": {
        "reportDir": "cypress/myreports/mocha",
        "quiet": true,
        "overwrite": false,
        "html": false,
        "json": true
    }
}